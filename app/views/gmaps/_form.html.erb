<%= stylesheet_link_tag :gmaps, :media => "all" %>

<%= simple_form_for(@gmap) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :title %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
